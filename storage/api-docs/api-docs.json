{
    "openapi": "3.0.0",
    "info": {
        "title": "SPOTRACK API",
        "description": "L5 Swagger OpenApi para la interface de recepcion de rutas generadas por SpoTrack",
        "contact": {
            "email": "jesus.calle.ruiz8@gmail.com"
        },
        "version": "1.0"
    },
    "paths": {
        "/api/route": {
            "post": {
                "tags": [
                    "Route"
                ],
                "summary": "Almacenar ruta de un usuario",
                "operationId": "9a5bc2f4f46137c1c45a2dbada9c412b",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "user_id",
                                    "name",
                                    "distance",
                                    "duration",
                                    "path",
                                    "average_speed",
                                    "max_speed"
                                ],
                                "properties": {
                                    "user_id": {
                                        "type": "integer",
                                        "example": "1"
                                    },
                                    "name": {
                                        "type": "string",
                                        "maxLength": 32,
                                        "minLength": 1,
                                        "example": "Mi primera ruta"
                                    },
                                    "distance": {
                                        "type": "string",
                                        "maxLength": 32,
                                        "minLength": 1,
                                        "example": "10km"
                                    },
                                    "duration": {
                                        "type": "string",
                                        "maxLength": 32,
                                        "minLength": 1,
                                        "example": "1h 30m"
                                    },
                                    "path": {
                                        "type": "string",
                                        "maxLength": 32,
                                        "minLength": 1,
                                        "example": "?"
                                    },
                                    "average_speed": {
                                        "type": "string",
                                        "maxLength": 32,
                                        "minLength": 1,
                                        "example": "10km/h"
                                    },
                                    "max_speed": {
                                        "type": "string",
                                        "maxLength": 32,
                                        "minLength": 1,
                                        "example": "30km/h"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Resultado del registro de lal ruta",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "error": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Route has been recorded"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error asociado al registro de la ruta",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "error": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Missing required parameter route"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/route/{user_id}": {
            "get": {
                "tags": [
                    "Route"
                ],
                "summary": "Obtener todas las rutas de un usuario",
                "operationId": "bb49cdd91e551da5ad3674e0b65455c9",
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "description": "ID del usuario para filtrar las rutas",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Listado de rutas del usuario",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "user_id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "Mi primera ruta"
                                            },
                                            "distance": {
                                                "type": "string",
                                                "example": "10km"
                                            },
                                            "duration": {
                                                "type": "string",
                                                "example": "1h 30m"
                                            },
                                            "path": {
                                                "type": "array",
                                                "items": {
                                                    "properties": {
                                                        "lat": {
                                                            "type": "number",
                                                            "format": "float",
                                                            "example": 40.4639
                                                        },
                                                        "lng": {
                                                            "type": "number",
                                                            "format": "float",
                                                            "example": -3.8095
                                                        }
                                                    },
                                                    "type": "object"
                                                }
                                            },
                                            "average_speed": {
                                                "type": "string",
                                                "example": "10km/h"
                                            },
                                            "max_speed": {
                                                "type": "string",
                                                "example": "30km/h"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error al obtener las rutas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "error": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Error retrieving routes"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Route",
            "description": "Route"
        }
    ]
}